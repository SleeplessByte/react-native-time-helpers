{
  "name": "react-native-time-helpers",
  "version": "1.1.0",
  "author": "Derk-Jan Karrenbeld <derk-jan@delftsolutions.nl>",
  "license": "MIT",
  "description": "Time helpers for consistency across platforms when using react-native, for en-GB locale",
  "private": false,
  "main": "index.cjs.js",
  "module": "index.es.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "yarn build:module && yarn build:main && yarn build:dts",
    "build:main": "yarn tsc --module commonjs && move .\\index.js .\\index.cjs.js",
    "build:module": "yarn tsc --module es2015 && move .\\index.js .\\index.es.js",
    "build:dts": "yarn tsc --declaration --declarationMap --emitDeclarationOnly",
    "prepare": "yarn build"
  },
  "peerDependencies": {
    "react-native": "*"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/react-native": "^0.62.10",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "typescript": "^3.9.2"
  },
  "files": [
    "index.ts",
    "index.d.ts",
    "index.d.ts.map",
    "index.es.js",
    "index.cjs.js"
  ]
}
